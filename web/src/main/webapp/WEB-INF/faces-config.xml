<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
	<application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>en_US</supported-locale>
			<supported-locale>pt_BR</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>GlobalMessages</base-name>
			<var>msgs</var>
		</resource-bundle>
	</application>

	<navigation-rule>
		<from-view-id>/pages/*</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/pages/home.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>cadastroInsumo</from-outcome>

			<to-view-id>/pages/cadastro/insumos/cadastroInsumos.xhtml</to-view-id>

		</navigation-case>

		<navigation-case>
			<from-outcome>categoriaInsumo</from-outcome>
			<to-view-id>/pages/cadastro/insumos/categoriaInsumo.xhtml</to-view-id>

		</navigation-case>

		<navigation-case>
			<from-outcome>estoque</from-outcome>
			<to-view-id>/pages/controle/estoque.xhtml</to-view-id>

		</navigation-case>
		<navigation-case>
			<from-outcome>prospeccao</from-outcome>
			<to-view-id>/pages/atendimento/prospeccao.xhtml</to-view-id>

		</navigation-case>

	</navigation-rule>

	<navigation-rule>
		<from-view-id>/pages/home.xhtml</from-view-id>

		<navigation-case>
			<from-outcome>estoque</from-outcome>
			<to-view-id>/pages/controle/estoque.xhtml</to-view-id>
		</navigation-case>

	</navigation-rule>
</faces-config>
